<div class="row">
        <div class="col-md-6">
            <h4>Others who added: <%= @song.artist %> - <%= @song.title %></h4>
            <table class="table">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Times Added</th>
                    </tr>
                </thead>
                <tbody>
                    <% @users.each {|user| %>
                        <tr>
                            <td><%= user[0][0] %></td>
                            <td><%= user[1] %></td>
                        </tr>
                    <% } %>
                </tbody>
            </table>
        </div>
        <div class="col-md-6">
            <a href="/songs">Dashboard</a> | 
            <a href="/sessions/logout">Logout</a>
        </div>
    </div>